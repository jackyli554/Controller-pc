  <Control name="label_Description" text="Descrição do proplema:"/>
  <Control name="label_Recommend" text=" Recomendação :"/>
  <Control name="label_FurtherDetails" text="Os fundos aparecem quando ocorrer os problemas, tal como o sistema operativo , os passos operados do software, que vai oferecer grande ajuda para resolver o problema."/>
  <Control name="crystalButton_SendEmail" text="Enviar"/>
  <Control name="crystalButton_SaveErrorAsTxt" text="Salvar os dados errados como um documento "/>
  <Control name="crystalButton_Cancel" text="Cancelar"/>
</Controls>
</Form>

</Resource>